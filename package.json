{
  "name": "rag-evaluator-app-root",
  "private": true,
  "version": "1.0.0",
  "description": "Root orchestrator for RAG Evaluator App",
  "scripts": {
    "postinstall": "npm install --prefix frontend",
    "dev:backend": "uvicorn main:app --reload --app-dir backend",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm:dev:*\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
